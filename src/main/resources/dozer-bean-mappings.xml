<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">

    <configuration>
        <custom-converters>
            <converter type="br.ifes.leds.sincap.web.converter.TelefoneToTelefoneDTOConverter">
                <class-a>br.ifes.leds.sincap.controleInterno.cln.cdp.Telefone</class-a>
                <class-b>br.ifes.leds.sincap.web.model.TelefoneDTO</class-b>
            </converter>
        </custom-converters>
    </configuration>
    
    <mapping>
        <class-a>br.ifes.leds.sincap.controleInterno.cln.cdp.Notificador</class-a>
        <class-b>br.ifes.leds.sincap.web.model.NotificadorDTO</class-b>
        <field
            custom-converter="br.ifes.leds.sincap.web.converter.InstituicaoNotificadoraSetToIdArrayConverter">
            <a>instituicoesNotificadoras</a>
            <b>instituicoesNotificadoras</b>
        </field>
    </mapping>
    
</mappings>