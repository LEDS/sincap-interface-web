<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
    http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
    <application>
        <!-- Mensagens para as páginas -->
        <resource-bundle>
            <base-name>br.ifes.leds.sincap.message.messages</base-name>
            <var>msg</var>
        </resource-bundle>

        <!-- Nomes das seções dos facelets -->
        <resource-bundle>
            <base-name>br.ifes.leds.sincap.facelets.facelets-sections</base-name>
            <var>sects</var>
        </resource-bundle>
    </application>
</faces-config>
